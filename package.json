{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/jit": "^0.1.17",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21",
    "laravel-mix": "^6.0.12",
    "lodash": "^4.17.19",
    "postcss": "^8.2.8",
    "tailwindcss": "^2.0.4"
  },
  "browserslist": [
    "> 1%"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "rules": {
      "indent": [
        "error",
        2
      ],
      "quotes": [
        "warn",
        "single"
      ],
      "comma-dangle": [
        "warn",
        "always-multiline"
      ]
    }
  }
}
