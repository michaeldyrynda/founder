{
  "name": "dyrynda/founder",
  "type": "project",
  "description": "Founder is a starter project for your Laravel applications.",
  "keywords": [
    "laravel",
    "starter"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2.5",
    "dyrynda/laravel-make-user": "^7.0",
    "fideloper/proxy": "^4.2",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^6.3",
    "hemp/presenter": "^2.0",
    "laracasts/flash": "^3.0",
    "laravel/framework": "^7.22",
    "laravel/horizon": "^4.0",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^2.0",
    "sentry/sentry-laravel": "^1.7"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "beyondcode/laravel-dump-server": "^1.0",
    "ergebnis/composer-normalize": "^2.6",
    "facade/ignition": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.0",
    "fzaninotto/faker": "^1.9.1",
    "laravel/telescope": "^3.2",
    "mockery/mockery": "^1.3.1",
    "nunomaduro/collision": "^4.1",
    "orchestra/testbench": "^5.3",
    "phpunit/phpunit": "^9.0",
    "psalm/plugin-laravel": "^1.3",
    "rector/rector-prefixed": "^0.8.6",
    "sensiolabs/security-checker": "^6.0",
    "vimeo/psalm": "^3.12"
  },
  "suggest": {
    "dyrynda/laravel-model-uuid": "For applications that need globally-unique model identifiers",
    "livewire/livewire": "A front-end framework for Laravel.",
    "spatie/enum": "If enumerable values are required",
    "thenpingme/laravel": "Scheduled task monitoring built specifically for Laravel applications thenping.me"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "check-security": "vendor/bin/security-checker security:check",
    "cs-fix": "vendor/bin/php-cs-fixer fix",
    "normalise": "@composer normalize --indent-style space --indent-size 2 --ansi",
    "psalm": "vendor/bin/psalm",
    "rector": "vendor/bin/rector process app --ansi",
    "test": "vendor/bin/phpunit --colors=always"
  }
}
