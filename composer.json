{
    "name": "dyrynda/founder",
    "description": "Founder is a starter project for your Laravel applications.",
    "keywords": [
        "laravel",
        "starter"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "dyrynda/laravel-make-user": "^5.0",
        "fideloper/proxy": "^4.0",
        "hemp/presenter": "^2.0",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.8.*",
        "laravel/horizon": "^3.0",
        "laravel/tinker": "^1.0",
        "sentry/sentry-laravel": "^1.0"
    },
    "suggest": {
        "dyrynda/laravel-model-uuid": "For applications that need globally-unique model identifiers",
        "myclabs/php-enum": "If enumerable values are required"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/telescope": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5",
        "sensiolabs/security-checker": "^6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "cs-fix": "vendor/bin/php-cs-fixer fix",
        "check-security": "vendor/bin/security-checker security:check",
        "test": "vendor/bin/phpunit --colors=always",
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
